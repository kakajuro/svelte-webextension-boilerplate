{
  "name": "svelte-web-extension-template",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "chrome:dev": "cross-env TARGET=chrome vite dev",
    "firefox:dev": "cross-env TARGET=firefox vite dev",
    "edge:dev": "cross-env TARGET=edge vite dev",
    "chrome:build": "cross-env TARGET=chrome vite build",
    "firefox:build": "cross-env TARGET=firefox vite build",
    "edge:build": "cross-env TARGET=edge vite build",
    "build:all": "node buildAll $npm_config_zip $npm_config_keepunzipped"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^2.0.3",
    "@tsconfig/svelte": "^3.0.0",
    "@types/webextension-polyfill": "^0.10.0",
    "adm-zip": "^0.5.14",
    "cross-env": "^7.0.3",
    "cross-zip": "^4.0.1",
    "rimraf": "^5.0.7",
    "svelte": "^3.55.1",
    "svelte-check": "^2.10.3",
    "tslib": "^2.5.0",
    "typescript": "^5.3.2",
    "vite": "^4.0.0",
    "vite-plugin-web-extension": "^4.0.0",
    "webextension-polyfill": "^0.10.0",
    "zip-a-folder": "^3.1.7",
    "zip-dir": "^2.0.0"
  }
}
